package dynamicProgramming;

public class CoinChange {

	public static void main(String[] args) {
		int[] arr={1,2,3};
		int m=arr.length;
		int k=4;
		System.out.println(coinChange(arr,m,k);
	}
	public static int coinChange(int[] arr,int m,int k){
		if(k==0 || k==1){
			return k;
		}
		if(k<0){
			return 0;
		}
		
		return 
	}
}
