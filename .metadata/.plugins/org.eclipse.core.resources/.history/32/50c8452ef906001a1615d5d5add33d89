package treeDSA;

public class BinaryTreeToBST {
	
	static class Node{
		int key;
		Node left;
		Node right;
		
		public Node(int value){
			key=value;
			left=null;
			right=null;
		}
	}
	
	static class BinaryTree{
		Node root;
		
		public BinaryTree(int key){
			root=new Node(key);
		}
	}
	public static void main(String[] args) {
		

	}

}
